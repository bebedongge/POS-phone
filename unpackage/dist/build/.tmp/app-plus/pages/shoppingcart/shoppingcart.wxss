.xy-dialog {
  position: fixed;
  visibility: hidden;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 1500;
  -webkit-transition: visibility 200ms ease-in;
  -o-transition: visibility 200ms ease-in;
  transition: visibility 200ms ease-in;
}
.xy-dialog.xy-dialog__show {
    visibility: visible;
}
.xy-dialog__container {
    position: absolute;
    z-index: 1010;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
        -ms-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
    -webkit-transition: -webkit-transform 0.3s;
    transition: -webkit-transform 0.3s;
    -o-transition: transform 0.3s;
    transition: transform 0.3s;
    transition: transform 0.3s, -webkit-transform 0.3s;
    width: 540rpx;
    border-radius: 10rpx;
    background-color: #fff;
    overflow: hidden;
    opacity: 0;
    -webkit-transition: opacity 200ms ease-in;
    -o-transition: opacity 200ms ease-in;
    transition: opacity 200ms ease-in;
    padding-top: 30rpx;
}
.xy-dialog__header {
    position: relative;
    overflow: auto;
    -o-text-overflow: ellipsis;
       text-overflow: ellipsis;
    white-space: nowrap;
    padding: 0 24rpx 24rpx;
    line-height: 1.5;
    color: #303133;
    font-weight: bold;
    font-size: 40rpx;
    text-align: center;
}
.xy-dialog__content {
    position: relative;
    color: #303133;
    font-size: 36rpx;
    -webkit-box-sizing: border-box;
            box-sizing: border-box;
    line-height: 1.5;
}
.xy-dialog__content .modal-content {
      padding: 0 30rpx 30rpx;
}
.xy-dialog__content::after {
      content: " ";
      position: absolute;
      left: 0;
      bottom: -1px;
      right: 0;
      height: 1px;
      border-bottom: 1px solid #EBEEF5;
      -webkit-transform-origin: 0 0;
          -ms-transform-origin: 0 0;
              transform-origin: 0 0;
      -webkit-transform: scaleY(0.5);
          -ms-transform: scaleY(0.5);
              transform: scaleY(0.5);
}
.xy-dialog__footer {
    position: relative;
    overflow: auto;
    -o-text-overflow: ellipsis;
       text-overflow: ellipsis;
    white-space: nowrap;
    color: #303133;
    font-size: 36rpx;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -webkit-flex-direction: row;
        -ms-flex-direction: row;
            flex-direction: row;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
        -ms-flex-pack: justify;
            justify-content: space-between;
    -webkit-box-align: center;
    -webkit-align-items: center;
        -ms-flex-align: center;
            align-items: center;
}
.xy-dialog__footer .xy-dialog__btn {
      width: 50%;
      text-align: center;
      padding: 20rpx 0;
}
.xy-dialog__footer .xy-dialog__btn.xy-dialog__footer-cancel {
        color: #909399;
}
.xy-dialog__footer .xy-dialog__btn.xy-dialog__footer-confirm {
        color: #409EFF;
}
.xy-dialog__footer .xy-dialog__btn.xy-dialog__btn-row {
        width: 100%;
        text-align: center;
        padding: 20rpx 0;
}
.xy-dialog__footer .xy-dialog__btn.xy-dialog__btn-row.xy-dialog__footer-confirm {
          color: #409EFF;
}
.xy-dialog__mask {
    display: block;
    position: absolute;
    z-index: 1000;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.2);
    opacity: 0;
    -webkit-transition: opacity 200ms ease-in;
    -o-transition: opacity 200ms ease-in;
    transition: opacity 200ms ease-in;
}
.xy-dialog__show .xy-dialog__container, .xy-dialog__show .xy-dialog__mask {
    opacity: 1;
}


