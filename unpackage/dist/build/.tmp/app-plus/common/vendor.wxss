.HMmessages {width:100%;height:80rpx;position:fixed;top:-80rpx;left:0;z-index:997;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-pack:start;-webkit-justify-content:flex-start;-ms-flex-pack:start;justify-content:flex-start;-webkit-box-align:center;-webkit-align-items:center;-ms-flex-align:center;align-items:center;}
.HMmessages.show {-webkit-animation:show 0.3s linear both;animation:show 0.3s linear both;}
.HMmessages.hide {-webkit-animation:hide 0.3s linear both;animation:hide 0.3s linear both;}
@-webkit-keyframes show {0% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}100% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}}
@keyframes show {0% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}100% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}}
@-webkit-keyframes hide {0% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}100% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}}
@keyframes hide {0% {-webkit-transform:translateY(80rpx);transform:translateY(80rpx);opacity:1;}100% {-webkit-transform:translateY(0);transform:translateY(0);opacity:0;}}
.HMmessages .ico {width:80rpx;height:80rpx;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;-ms-flex-pack:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;-ms-flex-align:center;align-items:center;margin-left:2%;margin-right:-2%;}
.HMmessages .content {width:100%;height:80rpx;padding:0 2%;font-size:30rpx;white-space:nowrap;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-align:center;-webkit-align-items:center;-ms-flex-align:center;align-items:center;overflow:hidden;}
.HMmessages .closeBtn {width:60rpx;height:80rpx;display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-pack:center;-webkit-justify-content:center;-ms-flex-pack:center;justify-content:center;-webkit-box-align:center;-webkit-align-items:center;-ms-flex-align:center;align-items:center;margin-right:2%;}
.HMmessages .closeBtn .close {font-family:"HMfont" !important;font-size:30rpx;font-style:normal;}


